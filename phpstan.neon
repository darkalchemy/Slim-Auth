parameters:
    level: 8
    checkMissingIterableValueType: false
    bootstrapFiles:
        - config/constants.php
    paths:
        - app
        - tests
        - bin
        - bootstrap
        - config
        - routes
        - var
    ignoreErrors:
        - '#Call to an undefined static method Cartalyst\\Sentinel\\Native\\Facades\\Sentinel::(\w+)\(\)#'
        - '#Call to an undefined static method App\\Model\\User::(\w+)\(\)#'
